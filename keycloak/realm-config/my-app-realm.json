{
  "realm": "my-app",
  "enabled": true,
  "clients": [
    {
      "clientId": "my-app-client",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "my-app-secret",
      "redirectUris": [
        "http://localhost:4200/*"
      ],
      "webOrigins": [
        "http://localhost:4200"
      ],
      "publicClient": false,
      "protocol": "openid-connect",
      "authorizationServicesEnabled": true,
      "serviceAccountsEnabled": true,
      "directAccessGrantsEnabled": true
    }
  ],
  "roles": {
    "realm": [
      {
        "name": "user",
        "description": "Regular user role"
      },
      {
        "name": "admin",
        "description": "Administrator role"
      }
    ]
  },
  "defaultRoles": [
    "user"
  ],
  "users": [
    {
      "username": "admin",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Admin",
      "lastName": "User",
      "email": "admin@myapp.com",
      "credentials": [
        {
          "type": "password",
          "value": "admin123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "admin",
        "user"
      ],
      "clientRoles": {
        "my-app-client": [
          "manage-account",
          "view-profile"
        ]
      }
    },
    {
      "username": "user",
      "enabled": true,
      "emailVerified": true,
      "firstName": "Regular",
      "lastName": "User",
      "email": "user@myapp.com",
      "credentials": [
        {
          "type": "password",
          "value": "user123",
          "temporary": false
        }
      ],
      "realmRoles": [
        "user"
      ],
      "clientRoles": {
        "my-app-client": [
          "view-profile"
        ]
      }
    }
  ]
} 